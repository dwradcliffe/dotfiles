alias bx='bundle exec'
alias gs='git status'
alias gf='git fetch --prune'
alias gpm='git pull origin master'
alias gpl='git branch --merged | grep -v "\*" | xargs -n 1 git branch -d'
alias showhidden='defaults write com.apple.finder AppleShowAllFiles YES; killall Finder /System/Library/CoreServices/Finder.app'
alias hidehidden='defaults write com.apple.finder AppleShowAllFiles NO; killall Finder /System/Library/CoreServices/Finder.app'
alias sha256='shasum -a 256'
alias sknife='BUNDLE_GEMFILE=~/.chef/Gemfile bundle exec knife'
alias hl='pbpaste | highlight --syntax=js -O rtf | pbcopy'
# alias heroku='/usr/local/bin/heroku'
alias gen='dev cd genesis'
